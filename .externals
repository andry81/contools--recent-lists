repositories:

  _externals/contools:
    type:         git
    url:          https://github.com/andry81/contools
    version:      master
    subpaths:
      - __init__
      - _config
      - scripts/__init__
      - scripts/tools/__init__
      - scripts/tools/build
      - scripts/tools/cleanup
      - scripts/tools/exec
      - scripts/tools/registry
      - scripts/tools/std
      - scripts/tools/wmi
      - scripts/tools/xcopy
      - scripts/tools/ToolAdaptors/__init__
      - scripts/tools/ToolAdaptors/lnk

  _externals/contools--utils:
    type:         git
    url:          https://github.com/andry81/contools--utils
    version:      master
    subpaths:
      - __init__
      - _config
      - bin

  _externals/tacklelib:
    type:         git
    url:          https://github.com/andry81/tacklelib
    version:      master
    subpaths:
      - __init__
      - _config
      - vbs

  _externals/gnuwin32:
    type:         git
    url:          https://github.com/andry81-3dparty/external-tools--gnuwin32
    version:      20101227

  _externals/sysinternals:
    type:         git
    url:          https://github.com/andry81-3dparty/external-tools--sysinternals
    version:      20230412
